<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ce="clr-namespace:ProcrastinHater.Views.ChecklistElements"
	xmlns:vmce="clr-namespace:ProcrastinHater.ViewModels.ChecklistElements;assembly=ProcrastinHater.ViewModels"
	xmlns:vm="clr-namespace:ProcrastinHater.ViewModels;assembly=ProcrastinHater.ViewModels"
	xmlns:local="clr-namespace:ProcrastinHater.Views"
>
	<DataTemplate DataType="{x:Type vmce:GroupVM}">
		<ce:GroupView/>
	</DataTemplate>	

	<DataTemplate DataType="{x:Type vmce:TaskVM}">
		<ce:TaskView/>
	</DataTemplate>		

	<DataTemplate DataType="{x:Type vm:MainWindowVM}">
		<local:MainView/>
	</DataTemplate>
	
</ResourceDictionary>